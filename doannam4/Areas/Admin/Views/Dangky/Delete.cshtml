@model doannam4.Models.Dangky
@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<main id="main" class="main">
    <div class="pagetitle">
        <h2>Xác nhận xoá email đăng ký </h2>
    </div>

    <div class="form-row">
        <div class="form-group col-md-6">
            <label asp-for="DkyID">DkyID</label>
            <input type="text" class="form-control mb-3" asp-for="DkyID" disabled />
        </div>
        <div class="form-group col-md-6">
            <label asp-for="Emaildky">Email</label>
            <input type="text" class="form-control mb-3" asp-for="Emaildky" disabled />
        </div>
    </div>
    <!--Begin Form-->
    <form asp-action="Delete" mothod="post">
        <input type="hidden" asp-for="DkyID" />
        <a class="btn btn-lg btn-warning p-2 mr-5" href="javascript:history.back()">
            <i class="bi bi-arrow-left-circle"></i>Quay lại
        </a>
        <button type="submit" class="btn btn-lg btn-danger p-2">
            <i class="bi bi-trash-fill"></i>Xoá email đăng ký
        </button>
    </form>
    <!--End Form-->
</main>